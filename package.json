{
  "name": "azure-pipelines-test-report",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "webpack",
    "package": "tfx extension create --manifest-globs azure-devops-extension.json",
    "test": "cross-env TEST_REPORT_FILENAME=test-results.xml jest --verbose",
    "start:api": "node server.js",
    "start:ui": "webpack-dev-server --mode development",
    "start": "npm-run-all --parallel start:ui start:api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.167.51",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/q": "^1.5.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "base64-inline-loader": "^2.0.1",
    "body-parser": "^1.20.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit-reporter": "^1.1.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "sync-request": "^6.1.0",
    "tfx-cli": "^0.12.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|ts|tsx|jsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|azure-devops-ui|azure-devops-extension-sdk)/)"
    ],
    "testPathIgnorePatterns": [],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "preset": "ts-jest/presets/js-with-babel",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/**.tsx",
      "<rootDir>/src/**/**.ts"
    ],
    "coverageReporters": [
      "json",
      "html",
      "text",
      "cobertura"
    ]
  }
}
