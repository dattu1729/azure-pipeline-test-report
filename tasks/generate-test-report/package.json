{
  "name": "generate-test-report",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack",
    "test": "cross-env TEST_REPORT_FILENAME=test-results.xml jest --verbose"
  },
  "keywords": [],
  "author": "dattu",
  "license": "ISC",
  "dependencies": {
   "azure-pipelines-task-lib": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/q": "^1.5.5",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.3.1",
    "jest-junit-reporter": "^1.1.0",
    "ts-loader": "^9.4.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts)$": ["ts-jest", {"tsconfig": "tsconfig.json"}]
    },
    "transformIgnorePatterns": [
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "moduleNameMapper": {
    },
    "preset": "ts-jest/presets/js-with-babel",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": ["<rootDir>/**.ts"],
    "coverageReporters": [
      "json",
      "html",
      "text",
      "cobertura"
    ]
  }
}
